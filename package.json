{
  "name": "desenyon-infinite-idea",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "playwright test tests/performance",
    "test:visual": "playwright test tests/visual",
    "test:all": "npm run test:run && npm run test:e2e",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "postinstall": "prisma generate",
    "deploy:staging": "vercel --target staging",
    "deploy:production": "vercel --prod",
    "deploy:monitor": "node scripts/deployment-monitor.js",
    "deploy:monitor:staging": "node scripts/deployment-monitor.js staging",
    "deploy:rollback": "vercel rollback",
    "deploy:logs": "vercel logs",
    "deploy:env": "vercel env ls",
    "backup:database": "node scripts/backup-system.js database",
    "backup:export": "node scripts/backup-system.js export",
    "backup:full": "node scripts/backup-system.js full",
    "backup:cleanup": "node scripts/backup-system.js cleanup",
    "monitor:performance": "node -e \"require('./src/lib/monitoring/performance-monitor.ts').performanceMonitor.recordMemoryUsage()\"",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.56.0",
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.12.0",
    "@sentry/nextjs": "^9.40.0",
    "@sentry/node": "^9.40.0",
    "@sentry/react": "^9.40.0",
    "@tanstack/react-query": "^5.83.0",
    "@types/bull": "^3.15.9",
    "@types/nodemailer": "^6.4.17",
    "@upstash/redis": "^1.35.1",
    "@xyflow/react": "^12.8.2",
    "bull": "^4.16.5",
    "bullmq": "^5.56.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.6",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.525.0",
    "next": "15.4.1",
    "next-auth": "^4.24.11",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.10.1",
    "openai": "^5.10.1",
    "prisma": "^6.12.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.60.0",
    "redis": "^5.6.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.8",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.1",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.3",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
